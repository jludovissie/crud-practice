<h1> Marketing Degree Course List</h1>

<table>
    <thead>
        <th>Course</th>
        <th>Course Number</th>
        <th>Professor</th>
        <th>Add Class</th>
    </thead>
    <tbody> 
        <% @course.each do |t| %>
        <tr> 
            <td><%= t.name %></td>
            <td><%= t.number %></td>
            <td><%= t.professor %> </td>
            <td>ADD CLASS</td>
        </tr>
        <% end %> 
    </tbody>
</table> 